-- Ensure inj and prj are recognized as names in for loop controls.

domain Test with
   procedure Run;
add
   type A is array (Positive range 1..3) of Character;
   carrier Rep is A;

   procedure Run is
      P : % := prj("Foo");
   begin
      for I in inj(P)'Range loop
         inj(P)(I) := 'x';
      end loop;
   end Run;
end Test;

