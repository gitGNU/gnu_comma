-- Check that indefinite array types can be passed thru inj and prj.

domain Test with
   procedure Run;
add
   carrier Rep is String;

   function Make_String return String is
   begin
      return (1 .. 100 => 'x');
   end Make_String;

   procedure Run is
      C : Character := inj(prj(Make_String))(1);
   begin
      pragma Assert(C = 'x');
   end Run;

end Test;
