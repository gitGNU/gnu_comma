-- Ensure local and imported function declarations overload.

domain D with
   function Foo (X : Boolean) return Boolean;
add
   function Foo (X : Boolean) return Boolean is
   begin
      return X;
   end Foo;
end D;

domain E add

   type A is (An_A);

   function Foo (X : Boolean) return A is
   begin
      return An_A;
   end Foo;

   function Test return A is
      use D;
   begin
      return Foo(Foo(true));
   end Test;

end E;

