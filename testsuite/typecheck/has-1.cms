-- Simple signature/domain checks.

signature S end S;

signature S1 end S1;

signature S2 is S end S2;

generic
   abstract domain X is S;
signature T is S2 end T;

generic
   abstract domain X is S;
signature T1 is S1 and S2 end T1;

generic
   abstract domain X is S;
   abstract domain Y is T(X);
signature U is T1(Y) end U;

domain D is S2 end D;

domain E is T(D) end E;


