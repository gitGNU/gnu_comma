-- Ensure subroutine declarations with disjoint keyword sets can be redeclared.

signature S with
   procedure P (X : %);
   function F (X : %) return %;
end S;

signature T with
   procedure P (Y : %);
   function F (Y : %) return %;
end T;

signature U is
   S and
   T
with
   procedure P (Z : %);
   function F (Z : %) return %;
end U;
