-- Check that non-continuous aggregates are OK in presence of an others clause.

domain Test with
   procedure Run;
add
   type Idx is range 1..10;
   type Fixed_String is array (Idx) of Character;

   procedure Run is
      S : Fixed_String := (1..3 => 'x', 5..6 => 'y', others => 'z');
   begin end Run;
end Test;
