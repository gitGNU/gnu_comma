-- Ensure subroutine declarations with disjoint keyword sets are detected.
--
-- FIXME: The compiler generates poor diagnostics in this case.

signature S with
   procedure P (X : %);
   function F (X : %) return %;
end S;

signature T with
   procedure P (Y : %);         -- EXPECTED-ERROR: conflicts with declaration
   function F (Y : %) return %; -- EXPECTED-ERROR: conflicts with declaration
end T;

signature U is S and T end U;
