-- Ensure we are resolving procedures in the overloaded case.

signature S with
  type T is (A, B);
  procedure P (X : Boolean);
  procedure P (X : T);
end S;

generic
   abstract domain X is
      S;
   end X;
domain D add
   import X;
   procedure Test is
   begin
      P(true);
   end Test;
end D;
