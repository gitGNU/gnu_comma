-- Ensure parameters of mode "in" cannot be used in a mode "out" context.

domain D add
   procedure P (X : out Boolean) is
   begin
      X := true;
   end P;

   procedure Test (X : in Boolean) is
   begin
      -- EXPECTED-ERROR: "in" cannot be used in a "out" context
      P(X);
   end Test;
end D;

