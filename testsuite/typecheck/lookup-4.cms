-- MUST-FAIL:  Ensure local variables shadow imported functions.

domain D with
   function Foo return Bool;
add
   function Foo return Bool is
   begin
      return true;
   end Foo;
end D;

domain E add

   type Not_Bool is (X);

   function Test return Bool is
      Foo : Not_Bool := X;
      import D;
   begin
      return Foo;
   end Test;

end E;



