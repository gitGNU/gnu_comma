-- Ensure local variables shadow imported functions.

domain D with
   function Foo return Boolean;
add
   function Foo return Boolean is
   begin
      return true;
   end Foo;
end D;

domain E add

   type Not_Bool is (X);

   function Test return Boolean is
      Foo : Not_Bool := X;
      import D;
   begin
      return Foo;               -- EXPECTED-ERROR: Incompatible type
   end Test;

end E;



